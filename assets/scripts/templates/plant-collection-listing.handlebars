{{#each plantCollections as |plantCollection|}}
  {{log plantCollection}}
  <section data-id="{{plantCollection._id}}">
    <h3>
      {{#if plantCollection.name}}
        {{plantCollection.name}}
      {{else}}
        My Plant Collection
      {{/if}}
    </h3>
    <button data-id="{{plantCollection._id}}" class="btn-pc btn-danger">Delete</button>
  </section>

  {{/each}}
